<Window x:Class="Final_Project.MenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Final_Project"
        mc:Ignorable="d"
        Title="MenuWindow" Height="400" Width="250">
    <Grid>
        <Button x:Name="CamButton" Content="Cam's App" HorizontalAlignment="Left" Margin="75,100,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button x:Name="GwynButton" Content="Gwyn's App" HorizontalAlignment="Left" Margin="75,150,0,0" VerticalAlignment="Top" Width="75"/>
		<Button x:Name="ZachButton" Content="Zach's App" HorizontalAlignment="Left" Margin="75,200,0,0" VerticalAlignment="Top" Width="75" Click="Button_Zach_Click"/>
        <Button x:Name="ZackButton" Content="Zack's App" HorizontalAlignment="Left" Margin="75,250,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="SethButton" Content="Seth's App" HorizontalAlignment="Left" Margin="75,300,0,0" VerticalAlignment="Top" Width="75" Click="Button_Seth_Click"/>
        <Label Content="Select an App you would like to launch." HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
